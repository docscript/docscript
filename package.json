{
  "name": "docscript",
  "version": "0.0.10",
  "description": "Documentation generator for TypeScript built with ReactJS",
  "main": "./dist/docscript.js",
  "bin": {
    "docscript": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "show-example": "ln -s ../example/doc/generated ./dist/generated",
    "show-self": "ln -s ../doc/generated ./dist/generated",
    "gen-doc": "node ./dist/cli.js generate --sourceDir src",
    "gen-doc-lite": "node ./dist/cli.js generate --sourceDir src --ui '.' --mainPackage docscript --withoutSearch",
    "reflector": "./node_modules/.bin/webpack --config webpack.reflector.config.js --watch --progress",
    "reflector-build": "./node_modules/.bin/webpack --config webpack.reflector.config.js --progress --bail",
    "test": "npm run reflector-tests-build && ./node_modules/.bin/mocha dist/tests.js",
    "reflector-tests": "NODE_ENV=development ./node_modules/.bin/webpack --config webpack.reflector-tests.config.js --watch --progress",
    "reflector-tests-build": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.reflector-tests.config.js --progress --bail",
    "cli": "./node_modules/.bin/webpack --config webpack.cli.config.js --watch --progress",
    "cli-build": "./node_modules/.bin/webpack --config webpack.cli.config.js --progress --bail",
    "search-index": "./node_modules/.bin/webpack --config webpack.search-index.config.js --watch --progress",
    "search-index-build": "./node_modules/.bin/webpack --config webpack.search-index.config.js --progress --bail",
    "explorer": "./node_modules/.bin/webpack-dev-server --hot --inline --config webpack.explorer.config.js --progress --history-api-fallback",
    "explorer-build": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.explorer.config.js --progress --bail --display-modules --display-reasons -p",
    "explorer-watch": "./node_modules/.bin/webpack --config webpack.explorer.config.js --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docscript/docscript.git"
  },
  "keywords": [
    "typescript",
    "documentation"
  ],
  "author": "Stanislav Panferov <fnight.m@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/docscript/docscript/issues"
  },
  "homepage": "https://github.com/docscript/docscript",
  "devDependencies": {
    "autobind-decorator": "^1.3.3",
    "awesome-typescript-loader": "2.2.4",
    "bem-cn": "^2.1.2",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "empty-module": "0.0.2",
    "file-loader": "^0.9.0",
    "fuzzaldrin-plus": "^0.3.1",
    "git-hooks": "^1.1.1",
    "history": "^4.0.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "memory-fs": "^0.3.0",
    "mocha": "^3.0.2",
    "node-uuid": "^1.4.7",
    "nprogress": "^0.2.0",
    "null-loader": "^0.1.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-fontpath": "^0.3.0",
    "postcss-import": "8.1.0",
    "postcss-loader": "^0.13.0",
    "postcss-url": "^5.1.2",
    "precss": "^1.4.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hotkeys": "^0.9.0",
    "react-markdown": "^2.4.2",
    "react-redux": "^4.4.5",
    "react-sticky": "^5.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rscrollspy": "0.0.6",
    "shelljs": "^0.7.4",
    "standard-version": "^2.4.0",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.0-dev.0",
    "typescript": "^2.1.0-dev.20160913",
    "uglifyify": "^3.0.2",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.4"
  },
  "dependencies": {
    "doctrine": "^1.3.0",
    "es6-shim": "^0.35.1",
    "fs-extra": "^0.30.0",
    "fstream": "^1.0.10",
    "pkg-up": "^1.0.0",
    "request": "^2.74.0",
    "source-map-support": "^0.4.2",
    "tar": "^2.2.1",
    "temp": "^0.8.3",
    "yargs": "^5.0.0"
  }
}
