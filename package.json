{
  "name": "@tygen/monorepo",
  "version": "0.0.10",
  "private": true,
  "description": "Documentation generator for TypeScript",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "cd packages/tygen-html && DOCS=$(pwd)/../../docs yarn start",
    "build": "cd packages/tygen-reflector && yarn run build && cd ../tygen-html && yarn run build"
  },
  "_moduleAliases": {
    "@tygen/reflector": "lib/tygen-reflector/src/index.js",
    "@tygen/reflector/src": "lib/tygen-reflector/src",
    "@tygen/html": "packages/tygen-html/dist/index.js"
  },
  "alias": {
    "@tygen/reflector": "packages/tygen-reflector",
    "@tygen/html": "packages/tygen-html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tygen/tygen.git"
  },
  "keywords": [
    "typescript",
    "documentation"
  ],
  "author": "Stanislav Panferov <fnight.m@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tygen/tygen/issues"
  },
  "homepage": "https://github.com/tygen/tygen",
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "prettier": "^1.13.4",
    "jest": "^23.1.0",
    "@types/jest": "^23.0.0",
    "ts-jest": "^22.4.6",
    "module-alias": "^2.0.6"
  }
}