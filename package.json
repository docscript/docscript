{
  "name": "@tygen/monorepo",
  "version": "0.0.10",
  "private": true,
  "description": "Documentation generator for TypeScript",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "react": "cd packages/tygen-html && DOCS=$(pwd)/../../docs yarn start",
    "develop": "concurrently -p name -n \"tsc,react\" --allow-restart --restart-tries=0 --restart-after=100 \"yarn run react\" \"yarn exec tsc --watch --pretty\"",
    "react-build": "cd packages/tygen-html && yarn build"
  },
  "_moduleAliases": {
    "@tygen/reflector": "lib/tygen-reflector/src/index.js",
    "@tygen/reflector/src": "lib/tygen-reflector/src",
    "@tygen/html": "packages/tygen-html/dist/index.js"
  },
  "alias": {
    "@tygen/reflector": "packages/tygen-reflector",
    "@tygen/html": "packages/tygen-html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tygen/tygen.git"
  },
  "keywords": [
    "typescript",
    "documentation"
  ],
  "author": "Stanislav Panferov <fnight.m@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tygen/tygen/issues"
  },
  "homepage": "https://github.com/tygen/tygen",
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "prettier": "^1.11.1",
    "jest": "^22.4.3",
    "@types/jest": "^22.2.2",
    "ts-jest": "^22.4.2",
    "module-alias": "^2.0.6"
  }
}