{
  "name": "@docscript/monorepo",
  "version": "0.0.10",
  "private": true,
  "description": "Documentation generator for TypeScript",
	"workspaces": [
		"packages/*"
  ],
  "scripts": {
    "react": "cd packages/docscript-react && DOCS=$(pwd)/../../docs yarn start"
  },
  "_moduleAliases": {
    "@docscript/reflector": "lib/docscript-reflector/src/index.js",
    "@docscript/reflector/src": "lib/docscript-reflector/src",
    "@docscript/react": "lib/docscript-react/src/index.js",
    "@docscript/react/src": "lib/docscript-react/src"
  },
  "alias": {
    "@docscript/reflector": "packages/docscript-reflector",
    "@docscript/react": "packages/docscript-react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docscript/docscript.git"
  },
  "keywords": [
    "typescript",
    "documentation"
  ],
  "author": "Stanislav Panferov <fnight.m@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/docscript/docscript/issues"
  },
  "homepage": "https://github.com/docscript/docscript",
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "prettier": "^1.11.1",
    "jest": "^22.4.3",
    "@types/jest": "^22.2.2",
    "ts-jest": "^22.4.2",
    "module-alias": "^2.0.6"
  }
}
