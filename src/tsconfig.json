{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es6",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*.ts",
        "!./node_modules/**/*.tsx",
        "../node_modules/typescript/lib/typescriptServices.d.ts"
    ],
    "files": [
        "./actions.ts",
        "./defines.d.ts",
        "./doc/doc.ts",
        "./doc/gen.ts",
        "./lib/autobind.ts",
        "./models-i.ts",
        "./models.ts",
        "./records.ts",
        "./service.ts",
        "./state-i.ts",
        "./state.ts",
        "./stores/navigation.ts",
        "./stores/search.ts",
        "./typings/react-dom.d.ts",
        "./components/file/file.tsx",
        "./components/link/link.tsx",
        "./components/nav/nav.tsx",
        "./components/page/page.tsx",
        "./components/path/path.tsx",
        "./components/search/search.tsx",
        "./flux.tsx",
        "./index.tsx",
        "../node_modules/typescript/lib/typescriptServices.d.ts"
    ]
}
