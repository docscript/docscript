{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./actions.ts",
        "./components/interface/interface.ts",
        "./components/layout/layout.ts",
        "./components/navigator/navigator.ts",
        "./components/package-nav/package-nav.ts",
        "./components/page/page.ts",
        "./defines.d.ts",
        "./models-i.ts",
        "./models.ts",
        "./service.ts",
        "./state.ts",
        "./stores/navigation.ts"
    ]
}
