{
  "name": "@tygen/html",
  "version": "0.1.0",
  "description": "React frontend for tygen, typescript documentation generator",
  "keywords": [
    "typescript",
    "documentation",
    "generator"
  ],
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "tygen-html": "./bin/tygen-html.js"
  },
  "scripts": {
    "server-start": "nodemon --exec \"node -r module-alias/register -r source-map-support/register dist/server.js --server\"",
    "server-watch": "webpack --watch --config webpack.server.js",
    "client-watch": "webpack-dev-server --hot --config webpack.client.js",
    "start": "concurrently -p name -n \"server-watch,client-watch,server\" --allow-restart --restart-tries=0 --restart-after=100 \"yarn run server-watch\" \"yarn run client-watch\" \"yarn run server-start\"",
    "build": "webpack --config webpack.common.js"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/fuzzaldrin-plus": "^0.6.0",
    "@types/micro": "^7.3.1",
    "@types/minimist": "^1.2.0",
    "@types/react-dom": "^16.0.7",
    "@types/react-test-renderer": "^16.0.2",
    "@types/prettier": "^1.13.2",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4",
    "parcel-bundler": "^1.9.7",
    "source-map-support": "^0.5.9",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "react-hot-loader": "^4.3.8"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "fuzzaldrin-plus": "^0.6.0",
    "micro": "^9.3.3",
    "mime-types": "^2.1.20",
    "minimist": "^1.2.0",
    "prettier": "^1.14.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "remark-parse": "^5.0.0",
    "url-loader": "^1.1.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "react-test-renderer": "^16.5.0",
    "unified": "^7.0.0",
    "react-syntax-highlighter": "^8.0.1"
  }
}