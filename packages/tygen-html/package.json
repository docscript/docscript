{
  "name": "@tygen/html",
  "version": "0.1.0",
  "description": "React frontend for tygen, typescript documentation generator",
  "keywords": [
    "typescript",
    "documentation",
    "generator"
  ],
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "tygen-html": "./bin/tygen-html.js"
  },
  "scripts": {
    "server-start": "node dist/cli.js --server",
    "server-watch": "webpack --watch --progress --config webpack.server.js",
    "client-watch": "webpack-dev-server  --progress --hot --config webpack.client.js",
    "start": "concurrently -p name -n \"server-watch,client-watch,server\" \"yarn run server-watch\" \"yarn run client-watch\" \"yarn run server-start\"",
    "build": "webpack --config webpack.common.js"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/fuzzaldrin-plus": "^0.6.0",
    "@types/micro": "^7.3.1",
    "@types/minimist": "^1.2.0",
    "@types/mime-types": "^2.1.0",
    "@types/react-dom": "^16.0.7",
    "@types/react-test-renderer": "^16.0.2",
    "@types/prettier": "^1.13.2",
    "@types/webpack-env": "^1.13.6",
    "concurrently": "^4.0.1",
    "source-map-support": "^0.5.9",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "react-hot-loader": "^4.3.11",
    "svg-sprite-loader": "^4.1.2",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "react-icons": "^3.1.0",
    "webpack": "^4.20.2",
    "@babel/core": "^7.1.2",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "babel-loader": "^8.0.4",
    "style-loader": "^0.23.0",
    "css-loader": "^1.0.0",
    "ts-loader": "^5.2.1",
    "react-virtualized": "^9.20.1",
    "@types/react-virtualized": "^9.18.7",
    "file-loader": "^2.0.0",
    "extract-css-chunks-webpack-plugin": "^3.1.1",
    "terser-webpack-plugin": "^1.1.0",
    "webpack-node-externals": "^1.7.2",
    "linaria": "^1.0.0-alpha.5",
    "core-decorators": "^0.20.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "fuzzaldrin-plus": "^0.6.0",
    "micro": "^9.3.3",
    "mime-types": "^2.1.20",
    "minimist": "^1.2.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "remark-parse": "^5.0.0",
    "url-loader": "^1.1.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "react-test-renderer": "^16.5.2",
    "unified": "^7.0.0",
    "react-syntax-highlighter": "^8.0.1"
  }
}